YUI.add("fluid-share-explore-view",function(e,t){e.FlickrView.create(this.name,e.namespace("Views")["fluid-share-view"],[],{langBundles:this.details.langBundles,subClassName:t,initializer:function(e){return this.exploreId=e.exploreId,this.constructor.superclass.initializer.call(this,{extraShareButtonIconClassList:"share-explore-icon",showShareButtonLabel:!0,sharingDisabled:!1})},loadState:function(){return this.loadRequiredModels()},loadRequiredModels:function(){var e=this,t={id:this.exploreId,apiParams:{date:this.exploreId,page:1,per_page:15}};return this.appContext.getModel("explore-models",t).then(function(t){e.set("exploreModel",t),e.isDisabled=parseInt(t.getValue("totalItems"),10)<1,e.set("photos",t.getValue("photos").getList())},function(e){throw e})},buildContainer:function(){return this.constructor.superclass.buildContainer.call(this,{extraShareButtonIconClassList:"share-explore-icon",showShareButtonLabel:!0,sharingDisabled:this.isDisabled}),this},getGuestPassParams:function(){return{contentMarkedPublic:!0,shareToSpecific:{twitter:{canShare:!0},facebook:{canShare:!0},tumblr:{canShare:!0},pinterest:{canShare:!0},flickr:{canShare:!1}}}},prepareFacebookShareParameters:function(){return{url:this.get("exploreModel").getValue("url")}},prepareTwitterShareParameters:function(){var t=this.get("exploreModel");return{url:e.URLHelper.addProtocolToURL(t.getValue("url")),title:t.getValue("title")}},prepareTumblrShareParameters:function(){var e=this.get("exploreModel");return{title:e.getValue("title"),authorURL:e.getValue("url"),url:e.getValue("url"),displayPhotoURL:this.get("photos").map(function(e){return e.getLargestSize().url}),tags:"Flickr"}},preparePinterestShareParameters:function(){var t=this.get("exploreModel");return{title:t.getValue("title"),displayPhotoURL:this.get("photos").length>0?this.get("photos")[0].getLargestSize().url:null,shortURL:e.URLHelper.addProtocolToURL(t.getValue("url")),url:t.getValue("url"),count:1}},prepareLinkShareParameters:function(){var t=this.get("exploreModel"),r=this.get("photos");return{sizes:r.length>0?e.EmbedSizeHelper.getSizes(r[0]):void 0,url:t.getValue("url"),shortURL:e.URLHelper.addProtocolToURL(t.getValue("url")),supportsEmbedr:!0,isExplore:!0,title:t.getValue("title"),generateLink:!1}},getTitleIntlKey:function(){return"SHARE_EXPLORE_TO"}})},"@VERSION@",{requires:["flickr-view","flickr-promise","fluid-share-view","url-helper","embed-size-helper"],langBundles:["models"]});